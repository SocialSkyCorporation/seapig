{
  "name": "seapig",
  "version": "0.8.1",
  "description": "SeaPig is converter from markdown to html.",
  "main": "js/seapig.js",
  "scripts": {
    "start": "electron .",
    "test": "mocha",
    "lint": "eslint",
    "lint-all": "eslint js",
    "clean": "rimraf releases *.log external",
    "build-dep": "node scripts/build-dep.js",
    "package:darwin": "electron-packager --ignore='^/ace-builds' --ignore='^/scripts' --platform=darwin --arch=all --prune --asar=true --out=releases --icon=seapig.icns .",
    "package:win32": "electron-packager --ignore='^/ace-builds' --ignore='^/scripts' --platform=win32 --arch=all --prune --asar=true --out=releases --icon=seapig.ico .",
    "package:linux": "electron-packager --ignore='^/ace-builds' --ignore='^/scripts' --platform=linux --arch=all --prune --asar=true --out=releases ."
  },
  "author": "Yasumichi Akahoshi <yasumichi@vinelinux.org>",
  "license": "MIT",
  "dependencies": {
    "electron-json-storage": "^4.1.7",
    "highlight.js": "^9.15.10",
    "marked": "^0.7.0",
    "mithril": "^2.0.4",
    "uiflow": "git+https://github.com/tkrkt/uiflow.git#fix-argument-in-compile",
    "viz.js": "^1.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "electron": "^6.0.7",
    "electron-packager": "^14.0.5",
    "eslint": "^6.3.0",
    "jsdoc": "^3.6.3",
    "jsdom": "^15.1.1",
    "mermaid": "^8.2.6",
    "mocha": "^6.2.0",
    "power-assert": "^1.6.1",
    "rimraf": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yasumichi/seapig.git"
  },
  "keywords": [
    "markdown"
  ],
  "bugs": {
    "url": "https://github.com/yasumichi/seapig/issues"
  },
  "homepage": "https://github.com/yasumichi/seapig#readme"
}
